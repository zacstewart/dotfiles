[user]
	name = Zac Stewart
	email = zgstewart@gmail.com
	signingkey = 7AEDBFFA
[core]
	editor = vim
	excludesfile = /Users/zacstewart/.gitignore_global
	trustctime = false
[color]
	ui = true
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[web]
	browser = open
[browser "chrome"]
	cmd = open -a \"Google Chrome.app\"
[alias]
	hist = !sh -c \"git log --graph --pretty=format:\\\"%C(yellow)%h%Creset%C(blue)%d%Creset %C(white bold)%s%Creset %C(white dim)(by %an %ar)%Creset\\\" --all\"
	ctags = !.git/hooks/ctags
	cleanup = !git branch --merged | grep -v \"^\\*\" | grep -v \"^master\" | xargs git branch -d
[init]
	templatedir = ~/.git_template
[push]
	default = simple
[mergetool]
	keepBackup = false
[diff]
	tool = p4mergetool
[difftool]
	prompt = false
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[difftool "p4mergetool"]
	cmd = /Applications/p4merge.app/Contents/Resources/launchp4merge $LOCAL $REMOTE
[merge]
	tool = p4mergetool
[mergetool "p4mergetool"]
	cmd = /opt/homebrew-cask/Caskroom/p4merge/2014.3-1007540/p4merge.app/Contents/Resources/launchp4merge $PWD/$BASE $PWD/$REMOTE $PWD/$LOCAL $PWD/$MERGED
	trustExitCode = false
